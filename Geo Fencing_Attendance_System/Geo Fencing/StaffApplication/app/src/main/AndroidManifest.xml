<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.staffapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_router_con"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuesActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".StaffAttendanceActivity" />
        <activity android:name=".SendTimeActivity" />
        <activity android:name=".SubjectDetailActivity" />
        <activity android:name=".ViewAttendanceActivity" />
        <activity android:name=".DefulterActivity" />
        <activity android:name=".DefulterListActivity"></activity>
    </application>

</manifest>